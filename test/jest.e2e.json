{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@libs/types/(.*)$":"<rootDir>/../libs/types/src/$1",
    "^@libs/log/(.*)$":"<rootDir>/../libs/log/src/$1"
  }
}
